{% trans_default_domain "BusybeeStudentBundle" %}


{% block studentActivity_widget %}
    {{ form_widget(form) }}
{% endblock studentActivity_widget %}

{% block studentActivity_row %}
    {% set header = 'activity.students.title' %}
    {% set transDomain = 'BusybeeStudentBundle' %}
    {% include '@BusybeeHome/Page/panelStart.html.twig' %}
    {{ form_label(form) }}
    {{ form_widget(form) }}
    {{ form_errors(form) }}
    {{ miscButton({'additional': 'id="deSelectAll_' ~ name ~ '"', 'style': 'display: hidden; float: right', 'class': 'btn btn-default halflings halflings-unchecked', 'title': 'De-select All'})|raw }}
    {{ miscButton({'additional': 'id="selectAll_' ~ name ~ '"', 'class': 'btn btn-default halflings halflings-tick', 'title': 'Select All'})|raw }}
    {% include '@BusybeeHome/Page/panelEnd.html.twig' %}
{% endblock studentActivity_row %}