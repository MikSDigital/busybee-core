{# app/Resources/BusybeeBootstrapBundle/views/Form/errors.html.twig #}

	{% if errors is defined %}
        {% for error in errors %}
            <div class="row">
                <div class="col-lg-12">
                    <div class="alert alert-danger">
                        {{ error.getMessage()|trans ({}, 'BusybeeBootstrapBundle') }}
                    </div>
                </div>
            </div>
        {% endfor %}
    {% endif %}

